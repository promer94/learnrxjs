{
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console":"warn",
      "no-use-before-define":["error", {"functions":false, "classes":true}],
      "semi": ["error", "never"],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 120,
          "tabWidth": 2,
          "semi":false
        }
      ]
    }
  }